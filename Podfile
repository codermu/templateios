# Uncomment this line to define a global platform for your project
# platform :ios, '9.0'

target 'IosBlueprint' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for IosBlueprint

  # required pod for social media login
  # facebook
    pod 'FBSDKCoreKit'
    pod 'FBSDKShareKit'
    pod 'FBSDKLoginKit'
  # google
    pod 'GoogleSignIn'
  # Alamofire HTTP networking library
    pod 'Alamofire', '~> 4.0'
  # Swift Keychain Wrapper
    pod 'KeychainAccess'


  target 'IosBlueprintTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'IosBlueprintUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
